<suite name="Sample Playground Suite">
    <test name="Sample Playground Test" parallel="methods">
        <groups>
            <run>
                <include name="allTests"/>
            </run>
        </groups>
        <packages>
            <package name="com.seleniumToolkit.selenium.test.*"/>
        </packages>
    </test>
</suite>
